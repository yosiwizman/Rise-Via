{
  "name": "risevia-cannabis",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "validate": "node scripts/build-validation.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:ci": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 69",
    "lint:report": "eslint . --ext ts,tsx --format json --output-file eslint-report.json",
    "lint:summary": "eslint . --ext ts,tsx --format compact | grep -E '(error|warning)' | awk '{print $NF}' | sort | uniq -c | sort -rn",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:staged": "eslint --fix --max-warnings 0",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "security:check": "node scripts/security-check.js",
    "audit": "npm audit --audit-level=moderate",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u",
    "debt:report": "node scripts/tech-debt-reporter.js",
    "debt:track": "node scripts/track-violations.js",
    "debt:reduce": "npm run lint:fix && npm run format",
    "pre-commit": "lint-staged",
    "ci": "npm run type-check && npm run lint:ci && npm run test && npm run build",
    "ci:full": "npm run security:check && npm run ci && npm run debt:report",
    "prepare": "husky install",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "build:production": "npm run generate-sitemap && npm run build",
    "import-strains": "node scripts/import-strains.js",
    "generate-images": "node scripts/generate-images.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
