    "typescript": "~5.9.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^7.1.5",
    "vite-bundle-analyzer": "^1.2.1",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^3.2.4"
  }
}
